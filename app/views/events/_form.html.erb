


<%= form_for @event do |f| %>
  <br>
    <%= f.label :title %>
    <%= f.text_field :title, :autofocus => true %>
  <br>
    <%= f.label :description %>
    <%= f.text_field :description %>
  <br>
    <%= f.label :date %>
    <%= f.date_select :date %>
  <br>
  <%= f.label :user_id %>
  <%= f.text_field :user_id, :value => session[:user_id] %>
  <br>
    <%= f.submit %>

<% end %>
